{
  "name": "ioffline/laravel-discrete-api-base",
  "description": "Laravel API for discrete Frontend. Supports Sanctum token authentication.",
  "keywords": [
    "laravel",
    "discrete api",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Kabin",
      "email": "me@ioffline.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "IOF\\DiscreteApi\\Base\\": "src/"
    }
  },
  "require": {
    "php": "^8.2|^8.3",
    "ext-curl": "*",
    "ioffline/utils": "*",
    "laravel/framework": "^11.25",
    "laravel/sanctum": "^4.0"
  },
  "require-dev": {
    "laravel/pint": "^1.18"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    },
    "laravel": {
      "providers": [
        "IOF\\DiscreteApi\\Base\\Providers\\DiscreteApiBaseServiceProvider"
      ],
      "aliases": {}
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
